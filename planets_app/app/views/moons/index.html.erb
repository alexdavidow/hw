<ul class="indexshift">
	<% @moons.each do |moon| %>
	<li id="entries" class="translucent">
		Moon Name: <%= link_to moon.name, moon %><br>
		Moon Diameter (in km): <%= moon.diameter %><br>
		Distance from <%= link_to moon.solar_system.name, solar_system_path(moon.solar_system)%> (in km): <%= moon.distance %><br>
	</li><br>
	<% end %>
</ul>